{
    "name": "kra8/laravel-snowflake",
    "description": "Snowflake for Laravel.",
    "keywords": ["snowflake", "laravel"],
    "authors": [{
        "name": "Koki Asai",
        "email": "koki@asai.dev"
    }],
    "homepage" : "https://github.com/kra8/laravel-snowflake",
    "license": "MIT",
    "require": {
        "php": "^7.0|^7.2",
        "illuminate/support": "^6.0"
    },
    "require-dev" : {
		"phpunit/phpunit": "~8.0",
        "orchestra/testbench": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Kra8\\Snowflake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kra8\\Snowflake\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kra8\\Snowflake\\SnowflakeServiceProvider"
            ]
        }
    }
}
