{
    "name": "kra8/laravel-snowflake",
    "description": "Snowflake for Laravel and Lumen.",
    "keywords": ["snowflake", "Laravel", "Lumen"],
    "authors": [{
        "name": "Koki Asai",
        "email": "me@kra.dev"
    }],
    "homepage" : "https://github.com/kra8/laravel-snowflake",
    "license": "MIT",
    "scripts": {
        "test": [
            "phpunit"
        ]
    },
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^6.0"
    },
    "require-dev" : {
        "phpunit/phpunit": "~8.0",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Kra8\\Snowflake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kra8\\Snowflake\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kra8\\Snowflake\\Providers\\LaravelServiceProvider"
            ]
        }
    }
}
