{
    "name": "kra8/laravel-snowflake",
    "description": "Snowflake for Laravel and Lumen.",
    "keywords": [
        "php",
        "snowflake",
        "Laravel",
        "Lumen"
    ],
    "authors": [
        {
            "name": "Koki Asai",
            "email": "koki@asai.email"
        }
    ],
    "homepage": "https://github.com/kra8/laravel-snowflake",
    "license": "MIT",
    "scripts": {
        "test": [
            "phpunit"
        ],
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^6.3|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Kra8\\Snowflake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kra8\\Snowflake\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kra8\\Snowflake\\Providers\\LaravelServiceProvider"
            ]
        }
    }
}
